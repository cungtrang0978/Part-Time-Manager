<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView_Date"
        android:text="date"
        android:textSize="20sp"
        android:layout_marginStart="20dp"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView_TimeBegin"
        android:text="timeBegin"
        android:textSize="20sp"
        android:layout_marginStart="10dp"
        android:layout_toEndOf="@id/textView_Date"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView_TimeEnd"
        android:text="timeEnd"
        android:textSize="20sp"
        android:layout_marginStart="10dp"
        android:layout_toEndOf="@id/textView_TimeBegin"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView_Salary"
        android:text="salary"
        android:textSize="20sp"
        android:layout_marginStart="10dp"
        android:layout_toEndOf="@id/textView_TimeEnd"
        />

</RelativeLayout>